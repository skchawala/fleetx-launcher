{
  "name": "Fleetx Launcher",
  "description": "Easily access Fleetx and it's Media pages and more",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "48": "icons/icon@48x48.png",
    "128": "icons/icon@128x128.png"
  },
  "action": {
    "default_icon":"icons/icon@48x48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}